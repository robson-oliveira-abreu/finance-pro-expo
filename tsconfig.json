{
  "compilerOptions": {
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "declaration": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "src/core/*"
      ],
      "@infra/*": [
        "src/infra/*"
      ],
      "@ui/*": [
        "src/ui/*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}